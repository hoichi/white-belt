{
	// https://vscode-docs1.readthedocs.io/en/latest/getstarted/theme-color-reference/
	"name": "White Belt",
	"type": "light",
	"colors": {
		"editor.background": "#f6f4f0",
		"editorWidget.background": "#f6f4f0",
		"editor.foreground": "#484832",
		//
		"sideBar.background": "#e8e6e0",
		"editorHoverWidget.background": "#e8e6e0",
		"menu.background": "#e8e6e0",
		"focusBorder": "#dda517",
		"button.background": "#dda517",
		"button.hoverBackground": "#be9200",
		//
		"statusBar.background": "#f6f4f0",
		"statusBar.foreground": "#484832",
		"statusBar.border": "#e8e6e0",
		"statusBarItem.remoteBackground": "#f6f4f0",
		"statusBarItem.remoteForeground": "#484832",
		"editor.lineHighlightBackground": "#FFF",
		"editor.lineHighlightBorder": "#00000000",
		"editorLineNumber.foreground": "#c8c6c0",
		"editorBracketMatch.background": "#FFF",
		"editorBracketMatch.border": "#FFF",
		"editor.wordHighlightBackground": "#ffffff",
		"editor.wordHighlightStrongBackground": "#ffffff",
		//
		"errorLens.warningBackgroundLight": "#00000000",
		"errorLens.infoBackgroundLight": "#00000000",
		//
		"settings.headerForeground": "#7a6214",
		"settings.modifiedItemIndicator": "#92876c",
	},
	"tokenColors": [
		{
			"name": "Default",
			"scope": "source",
			"settings": {
				"foreground": "#233976"
			}
		},
		{
			"name": "Keywords and punctuation",
			"scope": [
				"keyword",
				"storage",
				"storage.type",
				"punctuation"
			],
			"settings": {
				"foreground": "#484832"
			}
		},
		{
			"name": "Entity names",
			"scope": [
				"entity.name.class",
				"entity.name.namespace",
				"support.class",
				"entity.other.inherited-class"
				// "support.function"
			],
			"settings": {
				"foreground": "#104c47"
			}
		},
		{
			"name": "Values",
			"scope": [
				"constant",
				"support.constant",
				"variable",
				"support.variable"
			],
			"settings": {
				"foreground": "#233976",
				//	"foreground": "#1a7f1f"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string",
				"meta.jsx.children"
			],
			"settings": {
				"foreground": "#448C27"
			}
		},
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#616156"
			}
		},
		{
			"name": "Annotations",
			"scope": [
				"variable.annotation",
				"punctuation.definition.annotation",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#616156"
			}
		},
		{
			"name": "Types",
			"scope": [
				"meta.type",
				"meta.return.type",
				"support.type",
			],
			"settings": {
				"foreground": "#7A3E9D"
			}
		},
		{
			"name": "Invalid - Illegal",
			"scope": "invalid.illegal",
			"settings": {
				"foreground": "#660000"
			}
		},
		{
			"name": "Exceptions",
			"scope": "entity.name.exception",
			"settings": {
				"foreground": "#660000"
			}
		},
		{
			"name": "Strings: Escape Sequences",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "Strings: Regular Expressions",
			"scope": "string.regexp",
			"settings": {
				"foreground": "#4B83CD"
			}
		},
		{
			"name": "Strings: Symbols",
			"scope": "constant.other.symbol",
			"settings": {
				"foreground": "#AB6526"
			}
		},
		{
			"name": "HTML: Doctype Declaration",
			"scope": [
				"meta.tag.sgml.doctype",
				"meta.tag.sgml.doctype string",
				"meta.tag.sgml.doctype entity.name.tag",
				"meta.tag.sgml punctuation.definition.tag.html"
			],
			"settings": {
				"foreground": "#AAAAAA"
			}
		},
		{
			"name": "HTML: Tags",
			"scope": [
				"meta.tag",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html"
			],
			"settings": {
				"foreground": "#104c47"
			}
		},
		{
			"name": "HTML: Tag Names",
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#4B83CD"
			}
		},
		{
			"name": "HTML: Attribute Names",
			"scope": [
				"meta.tag entity.other.attribute-name",
				"entity.other.attribute-name.html"
			],
			"settings": {
				"foreground": "#104c47"
			}
		},
		{
			"name": "HTML: Entities",
			"scope": [
				"constant.character.entity",
				"punctuation.definition.entity"
			],
			"settings": {
				"foreground": "#AB6526"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"meta.function",
				"meta.function-call"
			],
			"settings": {
				"foreground": "#233976",
			}
		},
		{
			"name": "CSS: Selectors",
			"scope": [
				"meta.selector",
				"meta.selector entity",
				"meta.selector entity punctuation",
				"entity.name.tag.css"
			],
			"settings": {
				"foreground": "#7A3E9D"
			}
		},
		{
			"name": "CSS: Property Names",
			"scope": [
				"meta.property-name",
				"support.type.property-name"
			],
			"settings": {
				"foreground": "#AB6526"
			}
		},
		{
			"name": "CSS: Property Values",
			"scope": [
				"meta.property-value",
				"meta.property-value constant.other",
				"support.constant.property-value"
			],
			"settings": {
				"foreground": "#448C27"
			}
		},
		{
			"name": "CSS: Important Keyword",
			"scope": "keyword.other.important",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup: Changed",
			"scope": "markup.changed",
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "Markup: Deletion",
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "Markup: Emphasis",
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markup: Error",
			"scope": "markup.error",
			"settings": {
				"foreground": "#660000"
			}
		},
		{
			"name": "Markup: Insertion",
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "Markup: Link",
			"scope": "meta.link",
			"settings": {
				"foreground": "#4B83CD"
			}
		},
		{
			"name": "Markup: Output",
			"scope": [
				"markup.output",
				"markup.raw"
			],
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "Markup: Prompt",
			"scope": "markup.prompt",
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "Markup: Heading",
			"scope": "markup.heading",
			"settings": {
				"foreground": "#AA3731"
			}
		},
		{
			"name": "Markup: Strong",
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup: Traceback",
			"scope": "markup.traceback",
			"settings": {
				"foreground": "#660000"
			}
		},
		{
			"name": "Markup: Underline",
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Markup Quote",
			"scope": "markup.quote",
			"settings": {
				"foreground": "#7A3E9D"
			}
		},
		{
			"name": "Markup Lists",
			"scope": "markup.list",
			"settings": {
				"foreground": "#4B83CD"
			}
		},
		{
			"name": "Markup Styling",
			"scope": [
				"markup.bold",
				"markup.italic"
			],
			"settings": {
				"foreground": "#448C27"
			}
		},
		{
			"name": "Markup Inline",
			"scope": "markup.inline.raw",
			"settings": {
				"fontStyle": "",
				"foreground": "#AB6526"
			}
		},
		{
			"name": "Extra: Diff Range",
			"scope": [
				"meta.diff.range",
				"meta.diff.index",
				"meta.separator"
			],
			"settings": {
				"foreground": "#434343"
			}
		},
		{
			"name": "Extra: Diff From",
			"scope": "meta.diff.header.from-file",
			"settings": {
				"foreground": "#434343"
			}
		},
		{
			"name": "Extra: Diff To",
			"scope": "meta.diff.header.to-file",
			"settings": {
				"foreground": "#434343"
			}
		}
	]
}